{
  "type": "module",
  "name": "@hazae41/tscousin",
  "description": "Zero-config supply-chain hardened TypeScript paths and extensions rewriter",
  "version": "1.0.17",
  "repository": "github:hazae41/tscousin",
  "author": "hazae41",
  "scripts": {
    "examine": "deno check ./src && deno lint ./src && deno test ./src",
    "prepare": "rm -rf ./out && tsc && deno -RW ./src/bin.ts",
    "version": "deno -RW ./vsync.ts && git add deno.json"
  },
  "files": [
    "./out"
  ],
  "exports": {
    ".": {
      "types": "./out/mod.d.ts",
      "import": "./out/mod.js"
    }
  },
  "bin": {
    "tscousin": "./out/bin.js"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "typescript": "^5.9.2"
  }
}