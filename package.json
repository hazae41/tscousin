{
  "type": "module",
  "name": "@hazae41/tscousin",
  "description": "Zero-config supply-chain hardened TypeScript paths and extensions rewriter",
  "version": "1.0.21",
  "repository": "github:hazae41/tscousin",
  "author": "hazae41",
  "scripts": {
    "examine": "deno lint ./src && deno check ./src && deno test ./src",
    "prepare": "rm -rf ./out && tsgo && deno -RWE ./src/bin.ts",
    "version": "deno -RW ./x.vsync.ts && git add deno.json",
    "synchro": "deno -RW ./x.dsync.ts && deno install"
  },
  "files": [
    "./out"
  ],
  "exports": {
    ".": {
      "types": "./out/mod.d.ts",
      "import": "./out/mod.js"
    }
  },
  "bin": {
    "tscousin": "./out/bin.js"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@typescript/native-preview": "^7.0.0-dev.20251007.1"
  }
}
